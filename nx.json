{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "format"
        ]
      }
    }
  },
  "targetDefaults": {
    "prepare": {
      "dependsOn": [
        "^prepare"
      ],
      "outputs": ["dist"]
    },
    "test": {
      "dependsOn": [
        {
          "target": "prepare",
          "projects": "dependencies"
        },
        "^test"
      ]
    }
  }
}
