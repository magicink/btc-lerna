{
  "name": "generator-btc-rollup",
  "version": "1.0.0",
  "private": true,
  "description": "A custom RollupJS configuration.",
  "license": "UNLICENSED",
  "author": "Brandon Tom <magicink@gmail.com>",
  "main": "app/index.js",
  "files": [
    "app/"
  ],
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "format": "npx sort-package-json && prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "quoteProps": "as-needed",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "devDependencies": {
    "generator-btc-babel": "file:../generator-btc-babel",
    "prettier": "2.3.2",
    "yeoman-generator": "^5.3.0"
  }
}
