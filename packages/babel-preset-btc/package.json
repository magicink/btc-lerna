{
  "name": "babel-preset-btc",
  "version": "1.0.0",
  "homepage": "https://github.com/magicink/btc-lerna#readme",
  "private": false,
  "bugs": {
    "url": "https://github.com/magicink/btc-lerna/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magicink/btc-lerna.git"
  },
  "license": "ISC",
  "author": "Brandon Tom <magicink@gmail.com>",
  "scripts": {
    "format": "npx sort-package-json && prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "quoteProps": "as-needed",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "dependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.14.5",
    "@babel/plugin-transform-block-scoping": "^7.15.3",
    "@babel/plugin-transform-classes": "^7.15.4",
    "@babel/plugin-transform-destructuring": "^7.14.7",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/plugin-transform-spread": "^7.15.8",
    "@babel/plugin-transform-template-literals": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-flow": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.15.4",
    "core-js": "^3.18.3",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "prettier": "2.4.1"
  }
}
