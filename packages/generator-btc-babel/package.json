{
  "name": "generator-btc-babel",
  "version": "1.0.0",
  "private": true,
  "description": "Generates a Babel configuration file and dependencies.",
  "license": "ISC",
  "author": "Brandon Tom <magicink@gmail.com>",
  "main": "app/index.js",
  "files": [
    "app/"
  ],
  "scripts": {
    "format": "npx sort-package-json && prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "quoteProps": "as-needed",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "dependencies": {
    "yeoman-generator": "^5.3.0"
  },
  "devDependencies": {
    "prettier": "2.3.2"
  }
}
