{
  "version": "1.0.20",
  "command": {
    "bootstrap": {
      "hoist": [
        "prettier",
        "rimraf"
      ]
    },
    "publish": {
      "ignoreChanges": [
        "*-lock.json",
        "*.lock",
        "*.md",
        "*.test.js",
        ".gitignore",
        ".prettierignore",
        "test.js"
      ]
    }
  },
  "npmClient": "npm",
  "packages": [
    "packages/*"
  ]
}
